{% extends 'backend.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-md-6"> 
    
                <h2>{{'Club'|trans}}</h2>

                {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
                    {{ form_widget(form) }}
                    <div class="actions">
                        <input type="submit" value="{{'Guardar'|trans}}" class="btn btn-primary" />
                        <a href="{{ path('backend_club_index') }}" class="btn btn-default" role="button">{{'Cancelar'|trans}}</a>
                    </div>
                {{ form_end(form) }}
                
            </div>
        </div>
    </div>            
{% endblock %}